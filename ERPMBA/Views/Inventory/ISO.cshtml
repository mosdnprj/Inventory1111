@model ERPMBA.Models.ISO
@{
    ViewBag.Title = "ISO";
}
@Html.Partial("SO")
<div style="float:left; width:60%;height:100%;padding-top:40px;padding-left:0px;">
    <div id="Address" style="width:100%;height:525px;">


        <form>
            <h3> <span class="glyphicon glyphicon-import"></span>Import Orders</h3>
            <hr />
            <table>
                <tr>
                    <td>
                        Store View
                    </td>
                    <td>
                        <select name="storeview" size="1" style="width:300px;height:30px;" class="form-control">
                            <option>Default Store View</option>
                            <option></option>
                            <option></option>
                            <option></option>
                            <option></option>
                        </select>
                    </td>
                </tr>
                <tr>
                    <td>
                        Import Order Limit
                    </td>
                    <td>
                        <select name="orderlimit" size="1" style="width:300px;height:30px;" class="form-control">
                            <option>10</option>
                            <option>20</option>
                            <option>30</option>
                            <option>40</option>
                            <option>50</option>
                        </select>
                    </td>
                </tr>
                <tr>
                    <td>Order CSV File</td>
                    <td>
                        <input type="file" id="myFile" class="form-control">
                        <script>
                function myFunction() {
                    var x = document.getElementById("myFile");
                    x.disabled = true;
                }
                        </script>
                    </td>

                </tr>
            </table>
            <br />
            <div style="padding-left:120px;">Note: Strictly Recommended to import CSV file</div>
            <table>
                <tr>
                    <td>
                        <button id="submit" name="submit" class="btn btn-primary" value="1">Import</button>
                    </td>
                </tr>
            </table>



        </form>
    </div>
</div>

