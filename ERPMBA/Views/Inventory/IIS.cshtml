@model ERPMBA.Models.IIS
@{
    ViewBag.Title = "IIS";
}

@Html.Partial("IS")
<div style="float:left; width:60%;height:100%;padding-top:40px;padding-left:0px;">
    <div id="Address" style="width:100%;height:525px;">
        <form>
            <h3> <span class="glyphicon glyphicon-import"></span>Import Invoice</h3>
            <hr />
            <table>
                <tr>
                    <td>Upload File</td>
                    <td>
                        <input type="file" id="myFile" class="form-control">
                        <script>
                function myFunction() {
                    var x = document.getElementById("myFile");
                    x.disabled = true;
                }
                        </script>
                    </td>

                </tr>
                <tr>
                    <td>
                        <button id="submit" name="submit" class="btn btn-primary" value="1">Import</button>
                    </td>
                </tr>
            </table>
        </form>
    </div>
</div>