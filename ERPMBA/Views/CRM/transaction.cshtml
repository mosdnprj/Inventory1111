@model ERPMBA.Models.transaction
@{
    ViewBag.Title = "transaction";
}
@Html.Partial("Home")


<div class="tab-content" style=" float:left; width:60%;height:100%;">
    <div id="transaction" class="tab-pane fade in active" style="color: #0026ff; width:100%;height:100%;">

        <form name="contactForm" method="post" action="" class="form-horizontal" role="form">

            <div class="col-lg-12">
                <div id="Contact" class="contact-form">
                    <div class="panel-heading">
                        <h3 align="center">Transaction Details</h3>
                    </div>
                    <table>
                        <tr>
                            <td>
                                <div class="input-group">
                                    <span class="input-group-addon">memberid</span>
                                    @Html.TextBoxFor(model => model.memberid, new { @class = "form-control", placeholder = "memberid" })
                                    <!--<input type="text" class="form-control" placeholder="Imie" id="inputName" required="required">-->
                                </div>
                            </td>
                        </tr>

                </div>
            </div>

        </form>
        <table align="center">
            <tr>
                <td>
                    <button type="submit" class="btn btn-success" ">
                        submit

                    </button>
                </td>
                <td>
                    <button type="clear" class="btn btn-warning">
                        clear
                    </button>
                </td>
            </tr>
        </table>
    </div>
</div>